{
  "manifest_version": 2,
  "name": "Highlighter",
  "version": "0.1",
  "browser_action": {
    "default_popup": "login.html",
    "default_icon": "assets/icon.png"
  },
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "permissions": [
    "contextMenus",
    "storage",
    "http://localhost:3000/highlights"
  ]
}